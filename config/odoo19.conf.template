[options]
admin_passwd = {{ADMIN_PASSWD}}

db_host = 127.0.0.1
db_port = 5432
db_user = odoo
db_password = {{DB_PASSWORD}}

db_name = {{DB_NAME}}

addons_path = /opt/odoo/odoo{{ODOO_VERSION}}/odoo/addons,/opt/odoo/custom-addons

data_dir = /var/lib/odoo
logfile = /var/log/odoo/odoo{{ODOO_VERSION}}.log

proxy_mode = True
http_interface = 127.0.0.1
http_port = 8069